        "#5\n",
        "n = int(input())\n",
        "city_len = list(map(int, input().split()))\n",
        "city_price = list(map(int, input().split()))\n",
        "min_cost = 0\n",
        "\n",
        "for i in range(n-1):\n",
        "  if city_price[i] < city_price[0]:\n",
        "    city_price[0] = city_price[i]\n",
        "  min_cost += city_price[0] * city_len[i]\n",
        "\n",
        "print(min_cost)"
      ],
