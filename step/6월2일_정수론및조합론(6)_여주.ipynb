        "#9\n",
        "from math import factorial\n",
        "\n",
        "n = int(input())\n",
        "for _ in range(n):\n",
        "  n, m = map(int, input().split())\n",
        "  print(factorial(m)//(factorial(n)*factorial(m-n)))"
      ],
