        "#4\n",
        "n = int(input())\n",
        "stack = []\n",
        "result = []\n",
        "count = 0\n",
        "pos = True\n",
        "for _ in range(n):\n",
        "  num = int(input())\n",
        "\n",
        "  while count < num:\n",
        "    count += 1\n",
        "    stack.append(count)\n",
        "    result.append(\"+\")\n",
        "  \n",
        "  top = stack[-1]\n",
        "  if top == num:\n",
        "    stack.pop()\n",
        "    result.append('-')\n",
        "  else:\n",
        "    pos = False\n",
        "    break\n",
        "if pos == False:\n",
        "  print('NO')\n",
        "else:\n",
        "  for i in result:\n",
        "    print(i)"
